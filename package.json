{
  "name": "onpress",
  "productName": "onpress",
  "version": "0.1.0",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "lint": "eslint src",
    "comp": "export NODE_ENV=production&&electron-compile src",
    "comp:win": "set NODE_ENV=production&&electron-compile src",
    "comp:wind": "set DEBUG=*,-babel&&set NODE_ENV=production&&electron-compile src",
    "pack": "export NODE_ENV=production&&electron-forge package",
    "pack:win": "set NODE_ENV=production&&electron-forge package",
    "pack:wind": "set DEBUG=*,-babel&&set NODE_ENV=production&&electron-forge package",
    "make": "electron-forge make"
  },
  "keywords": [],
  "author": "kimor",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {},
      "electronWinstallerConfig": {
        "name": "onpress"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "onpress"
      }
    }
  },
  "dependencies": {
    "babel-runtime": "^6.22.0",
    "codemirror": "^5.22.2",
    "electron-compile": "^5.1.2",
    "electron-devtools-installer": "^2.0.1",
    "i18n": "^0.8.3",
    "less": "^2.7.2",
    "lodash": "^4.17.4",
    "markdown-it": "^8.2.2",
    "markdown-it-emoji": "^1.3.0",
    "normalize.css": "^5.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-split-pane": "^0.1.57",
    "semantic-ui-css": "^2.2.4",
    "semantic-ui-react": "^0.64.3"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-preset-babili": "0.0.10",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.16.0",
    "electron-compilers": "^5.4.0",
    "electron-prebuilt-compile": "1.4.14",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0"
  }
}
